# myclaw environment variables for docker-compose
# Copy this file to .env and fill in your values:
#   cp .env.example .env

# Required: AI provider API key
MYCLAW_API_KEY=sk-ant-...

# Optional: Telegram bot token
MYCLAW_TELEGRAM_TOKEN=

# Optional: Feishu bot credentials
MYCLAW_FEISHU_APP_ID=
MYCLAW_FEISHU_APP_SECRET=

# Optional: WeCom App (self-built app) credentials
MYCLAW_WECOM_APP_TOKEN=
MYCLAW_WECOM_APP_ENCODING_AES_KEY=
MYCLAW_WECOM_APP_RECEIVE_ID=
MYCLAW_WECOM_APP_CORP_ID=
MYCLAW_WECOM_APP_CORP_SECRET=
MYCLAW_WECOM_APP_AGENT_ID=
MYCLAW_WECOM_APP_API_BASE_URL=
